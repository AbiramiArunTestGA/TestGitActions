<InstructionDefinition Author="Unknown Author" Name="1E-Exchange-TachyonCore-RequestMachineCertificate" ReadablePayload="Request a machine certificate." Description="Requests a machine certificate from a trusted issuing certificate authority and stores it in the machine's personal store. This instruction makes use of a PowerShell script. Windows only.&#xD;&#xA;" InstructionType="Action" InstructionTtlMinutes="30" ResponseTtlMinutes="30" Version="2.3" xmlns="http://schemas.1e.com/Tachyon/InstructionDefinition/1.0">
  <Payload><![CDATA[@device= Device.GetSummary();
@device= SELECT OperatingSystemVersionText FROM @device WHERE OperatingSystemVersionText LIKE "%Windows 7%";
IF(@device)
NOTIMPLEMENTED "Not Implemented on Windows 7";
ENDIF;
@cert = NativeServices.RunCommand(CommandLine:"powershell.exe -command $cert = get-certificate -template Machine -CertStoreLocation cert:\\localmachine\\my;
$cert.certificate.thumbprint");
select @cert.Output as Thumbprint from @cert;]]></Payload>
  <SchemaJson><![CDATA[[
  {
    "Name": "Thumbprint",
    "Type": "String",
    "Length": 64
  }
]]]></SchemaJson>
